{
  "name": "esbuild-plugin-solid",
  "version": "0.6.0",
  "description": "Solid's integration for ESBuild",
  "keywords": [
    "solid",
    "solid-js",
    "esbuild",
    "plugin"
  ],
  "repository": "amoutonbrady/esbuild-plugin-solid",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "peerDependencies": {
    "esbuild": ">=0.20",
    "solid-js": ">= 1.0"
  },
  "dependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-typescript": "^7.18.6",
    "babel-preset-solid": "^1.6.9"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/node": "^20.12.8",
    "esbuild": "^0.20.2",
    "jiti": "^1.21.0",
    "serve": "^14.2.3",
    "solid-js": "^1.6.9",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "test": "jiti ./scripts/test && serve tests"
  }
}